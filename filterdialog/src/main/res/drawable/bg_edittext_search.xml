<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#1A000000"/>
            <corners android:radius="@dimen/radius_normal"/>
        </shape>
    </item>
    <item android:top="1dp" android:right="1dp" android:bottom="1dp" android:left="1dp">
        <shape>
            <solid android:color="@color/filterdialog_searchbox_background"/>
            <corners android:radius="@dimen/radius_normal"/>
            <stroke android:color="@color/filterdialog_searchbox_border" android:width="@dimen/stroke_normal"/>
        </shape>
    </item>
</layer-list>